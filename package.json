{
  "name": "teaserpaste-cli",
  "version": "0.6.5",
  "description": "CLI Client for TeaserPaste - View, create, and manage snippets.",
  "license": "MIT",
  "author": {
    "name": "TeaserPaste",
    "email": "contact@teaserverse.online",
    "url": "https://paste.teaserverse.online"
  },
  "main": "cli.js",
  "bin": {
    "tp": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TeaserPaste/cli"
  },
  "scripts": {
    "start": "node cli.js",
    "build-win": "bun build ./cli.js --compile --outfile dist/tp",
    "build-linux": "bun build ./cli.js --compile --target=bun-linux-x64 --outfile dist/tp-linux",
    "build-mac": "bun build ./cli.js --compile --target=bun-darwin-x64 --outfile dist/tp-macos"
  },
  "dependencies": {
    "clipboardy": "^5.0.0",
    "inquirer": "^12.10.0",
    "keytar": "^7.9.0",
    "node-fetch": "^2.7.0"
  },
  "preferGlobal": true
}
